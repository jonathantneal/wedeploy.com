{namespace docs}

/**
 * @param content
 * @param page
 */
{template .render}
  <div class="main">
    <main class="content guide">
      <div class="docs">
        {call .topbar data="all" /}

        {call Sidebar.render}
          {param page: $page /}
        {/call}

        <div class="sidebar-offset">
          {$content}
          {call Newsletter.render /}
        </div>
      </div>
    </main>
  </div>
{/template}

/**
 */
{template .topbar}
  <nav class="topbar topbar-docs topbar-light topbar-large">
    {call .logo data="all" /}
    {call .menu data="all" /}
  </nav>
{/template}

